{
  "name": "website-ssr-mobile",
  "version": "4.3.2",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "build:client": "cross-env TARGET_ENV=client vue-cli-service build",
    "build:server": "cross-env TARGET_ENV=server vue-cli-service build --no-clean",
    "build:spa": "npm run build",
    "build:ssr": "npm run build:client && npm run build:server",
    "postbuild:ssr": "rm server/public/index.html",
    "dev:spa": "npm run serve",
    "dev:ssr": "cross-env NODE_ENV=development nodemon -r dotenv/config server/app.js",
    "start": "cross-env NODE_ENV=production node -r dotenv/config server/app.js",
    "pre-release": "git checkout develop; git pull; git checkout master; git merge develop;",
    "release-patch": "npm run pre-release && npm version patch -m 'tag: 版本更新至 %s patch' && git push --tags; git push;",
    "release-minor": "npm run pre-release && npm version minor -m 'tag: 版本更新至 %s minor' && git push --tags; git push;",
    "release-major": "npm run pre-release && npm version major -m 'tag: 版本更新至 %s major' && git push --tags; git push;"
  },
  "dependencies": {
    "@sentry/node": "^5.22.3",
    "@sentry/tracing": "^5.22.3",
    "async-lock": "^1.2.4",
    "blueimp-md5": "^2.16.0",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "recaptcha-v3": "^1.8.0",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-i18n": "^8.17.3",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.2.0",
    "vue-server-renderer": "^2.6.11",
    "vue-slider-component": "^3.2.2",
    "vuex": "^3.4.0",
    "vuex-router-sync": "^5.0.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-e2e-cypress": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-pwa": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-unit-mocha": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.3",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "chai": "^4.1.2",
    "cookie-parser": "^1.4.5",
    "crc-32": "^1.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "express-redis-cache": "^1.1.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "http-proxy-middleware": "^1.0.5",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "less-vars-to-js": "^1.3.0",
    "mobile-detect": "^1.4.4",
    "patpat-ui": "^1.1.3",
    "uuid": "^8.3.0",
    "vue-l10n": "^0.1.2",
    "vue-template-compiler": "^2.6.11"
  }
}
